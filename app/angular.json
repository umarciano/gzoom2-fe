{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gzoom2": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "moment-timezone"
           ],
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/ionicons-npm/css/ionicons.css",
              "node_modules/fullcalendar/dist/fullcalendar.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/brands.css",
              "node_modules/@fortawesome/fontawesome-free/css/regular.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.css",
              {
                "inject": false,
                "input": "src/styles/blue.scss",
                "bundleName": "blue"
              },
              {
                "inject": false,
                "input": "src/styles/violet.scss",
                "bundleName": "violet"
              },
              {
                "inject": false,
                "input": "src/styles/green.scss",
                "bundleName": "green"
              },
              {
                "inject": false,
                "input": "src/styles/blue_light.scss",
                "bundleName": "blue_light"
              },
              {
                "inject": false,
                "input": "src/styles/green_light.scss",
                "bundleName": "green_light"
              },
              {
                "inject": false,
                "input": "src/styles/violet_light.scss",
                "bundleName": "violet_light"
              },
              {
                "inject": false,
                "input": "src/styles/high_contrast.scss",
                "bundleName": "high_contrast"
              }
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jwt-decode/build/jwt-decode.js",
              "node_modules/lodash/lodash.js",
              "node_modules/moment/moment.js",
              "node_modules/moment-timezone/builds/moment-timezone-with-data.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/fullcalendar/dist/locale-all.js",
              "node_modules/chart.js/dist/chart.js",
              "src/js/style-switcher.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "dev": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gzoom2:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "gzoom2:build:dev"
            },
            "production": {
              "browserTarget": "gzoom2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gzoom2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jwt-decode/build/jwt-decode.js",
              "node_modules/lodash/lodash.js",
              "node_modules/moment/moment.js",
              "node_modules/moment-timezone/builds/moment-timezone-with-data.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/fullcalendar/dist/locale-all.js",
              "node_modules/chart.js/dist/chart.js"
            ],
            "styles": [
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/ionicons-npm/css/ionicons.css",
              "node_modules/fullcalendar/dist/fullcalendar.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/brands.css",
              "node_modules/@fortawesome/fontawesome-free/css/regular.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.css",
              {
                "inject": false,
                "input": "src/styles/green.scss",
                "bundleName": "green"
              },
              {
                "inject": false,
                "input": "src/styles/blue.scss",
                "bundleName": "blue"
              },
              {
                "inject": false,
                "input": "src/styles/violet.scss",
                "bundleName": "violet"
              }
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        }
      }
    },
    "gzoom2-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "gzoom2:serve"
          }
        }
      }
    }
  },
  "defaultProject": "gzoom2",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}